{
  "name": "@todo/pages-layout",
  "private": true,
  "main": "./dist/Layout.js",
  "types": "./src/Layout.tsx",
  "scripts": {
    "dev": "vite",
    "build": "pnpm build:export && pnpm build:css",
    "build:app": "tsc && vite build",
    "build:export": "tsup --format=cjs src/Layout.tsx",
    "build:css": "sass src/style.scss dist/layout.css",
    "preview": "vite preview"
  },
  "dependencies": {
    "@todo/data": "workspace:*",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-icons": "^4.3.1",
    "react-syntax-highlighter": "^15.5.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-syntax-highlighter": "^13.5.2",
    "@types/sass": "^1.43.1",
    "@vitejs/plugin-react": "^1.3.0",
    "sass": "^1.50.0",
    "tsup": "^5.12.5",
    "typescript": "^4.6.3",
    "vite": "^2.9.2"
  }
}
