{
  "scripts": {
    "dev:layout": "pnpm dev --filter ./packages/pages/layout",
    "build": "pnpm build:projects && pnpm build:packages && pnpm build:inject",
    "build:projects": "pnpm build --filter ./examples",
    "build:packages": "pnpm build --filter ./packages/utils && pnpm build --filter ./packages/pages",
    "build:inject": "pnpm build --filter ./scripts/build",
    "preview": "serve dist",
    "pretty": "prettier -w **/{src,scripts}/** --plugin-search-dir=."
  },
  "devDependencies": {
    "prettier": "^2.6.2",
    "prettier-plugin-organize-imports": "^2.3.4",
    "serve": "^13.0.2",
    "typescript": "^4.5.4"
  }
}
